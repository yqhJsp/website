/* pages/site/video/video.wxss */
page {
        background-color: #f5f5f5;
}

.session {
  margin: 0 auto;
  width: 100%;
}
.listBox{
  padding: 20rpx;
  position: relative;
}
.box {
       width:100%;
        height: 460rpx;
        display:inline-block;
        background-color: #fff;
        margin-top:20rpx
}

.listBox .box .boxImg {
        width: 100%;
        height: 350rpx;
        overflow: hidden;
}

.listBox .box .boxImg video{
        width: 100%;
        height: 100%;
}

.listBox .box .boxcoent {
        text-align: center;
        font-size: 26rpx;
        color: #333;
        padding: 20rpx;
}

.listBox .box .boxcoent view {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
        
}
.subhead{
 color:#999
}
.cover,.coverImg{
  height: 100%;
  position: relative;
  width: 100%
}
.playImg{
  position:absolute;
  left:0;
  top: 0;
  width: 100%;
  height: 350rpx;
}